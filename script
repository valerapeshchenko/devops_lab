eval "$(pyenv init -)"
git clone https://github.com/pyenv/pyenv-virtualenv.git $(pyenv root)/plugins/pyenv-virtualenv
pyenv global 3.6.4
pip install virtualenv
eval "$(pyenv init -)"
pyenv virtualenv v1
pyenv global 2.7.8
pyenv virtualenv v2
